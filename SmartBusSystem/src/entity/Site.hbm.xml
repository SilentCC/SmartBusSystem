<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="entity.Site" table="Site" >
	 		<id name="SiteID">
	 		
	 		</id>
	 			<property name="RouteID" length="30"/>
	 			<property name="SiteName" length="30"/>	 			
	 			<property name="PassageNum" length="30"/>
	 			<property name="Longitude" length="30"/>
	 			<property name="Latitude" length="30"/>
	 		 			 		
	 </class>
</hibernate-mapping>
